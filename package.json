{
  "name": "discord-tictactoe",
  "version": "2.0.0",
  "description": "A modern Discord Bot for playing Tic-Tac-Toe",
  "author": "Utarwyn <maximemalgorn@gmail.com>",
  "license": "Apache-2.0",
  "main": "dist/src/index.js",
  "bin": {
    "tictactoe": "dist/bin/tictactoe.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/utarwyn/discord-tictactoe"
  },
  "keywords": [
    "discord",
    "bot",
    "tictactoe",
    "game",
    "nodejs",
    "typescript"
  ],
  "devDependencies": {
    "@types/jest": "~25.2.1",
    "@types/node": "~12.12.38",
    "@typescript-eslint/eslint-plugin": "~2.32.0",
    "@typescript-eslint/parser": "~2.32.0",
    "eslint": "~7.0.0",
    "eslint-config-prettier": "~6.11.0",
    "eslint-plugin-jest": "~23.10.0",
    "jest": "~25.2.7",
    "prettier": "~2.0.5",
    "rimraf": "~3.0.2",
    "ts-jest": "~25.5.1",
    "tsutils": "~3.17.0",
    "typescript": "~3.8.3"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "discord.js": "^12.2.0",
    "tslib": "~1.11.2"
  },
  "engines": {
    "node": ">= 12.13 <13"
  },
  "scripts": {
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "format:check": "prettier --config .prettierrc --list-different '**/**.ts'",
    "format": "prettier --config .prettierrc --write '**/**.ts'",
    "lint": "eslint . --ext .ts,.tsx",
    "serve": "node dist/bin/tictactoe.js",
    "start": "npm run build && npm run serve",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "release": "npm run clean && npm run lint && npm run format && npm run build"
  }
}
